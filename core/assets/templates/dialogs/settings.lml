<dialog class="dialog-tan" title="@settingsTitle" titleAlign="center" oneColumn=true>
	@musicVolume
	<slider value="$getMusicVolume" onChange="setMusicVolume"/>
	@soundVolume
	<slider value="$getSoundVolume" onChange="setSoundVolume"/>
	<textButton onResult="restoreSettings" growX=true>@cancel</textButton>
	<textButton onResult="saveSettings" growX=true fillX=true>@save</textButton>
</dialog>