<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module>
	<source path="com/github/czyzby/gdx/idle" />
	<inherits name='com.github.czyzby.autumn.mvc.GdxAutumnMvc' />
	<inherits name='com.github.czyzby.autumn.gwt.GdxAutumnGwt' />

	<extend-configuration-property name="gdx.reflect.include" value="com.github.czyzby.gdx.idle" />
	<!-- GWT needed this after I switched to LibGDX reflection (probably due to asset injection): -->
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.graphics.g2d.TextureAtlas" />
</module>